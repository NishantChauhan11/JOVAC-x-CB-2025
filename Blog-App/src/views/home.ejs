<!DOCTYPE html>
<html>
<head>
  <title>Blog Home</title>
  <link rel="stylesheet" href="/style.css">
</head>
<body>
  <h1>All Blog Posts</h1>
  <a href="/new">Create New Post</a>
  <% blogs.forEach(blog => { %>
    <div>
      <h2><%= blog.title %></h2>
      <img src="<%= blog.imageUrl %>" width="200" />
      <p><%= blog.content %></p>
      <hr>
    </div>
  <% }) %>
</body>
</html>